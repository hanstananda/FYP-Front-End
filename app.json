{
  "expo": {
    "owner": "hanstananda",
    "description": "This project aims to develop a mobile application to detect and collect snake sightings for the general public on mobile platform, with the help of crowdsourcing. This application also includes a classifier that has been trained to detect and classify snakes that are commonly found in Asia. ",
    "name": "snake-classifier",
    "slug": "snake-classifier",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hanstananda.snakeclassifier"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "android": {
      "package": "com.hanstananda.snakeclassifier",
      "versionCode": 6,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBuEJ_JFXqCatqeRqBBcBXSUN_Tt5_HSTY"
        }
      }
    }
  }
}
